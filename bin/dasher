#!/usr/bin/env ruby

module Dash
  module Builtins
    def foo(args)
      puts "FOO"
    end
    module_function :foo

    def true(args)
      return 0
    end
    module_function :true

    alias_method ":", :true
    module_function ":"

    def false(args)
      return 1
    end
    module_function :false
  end
end

require 'dash/dash'

args = [$0]
args.concat(ARGV)

exit Dash.main(*args)

